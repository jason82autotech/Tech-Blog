<div class="col-md-12">
    <h2>Sign Up</h2>
    <form class="form signup-form">
    <div class="form-group">
        <label for="user-signup">Username:</label>
        <input class="form-input" type="text" id="user-signup" />
    </div>
    <div class="form-group">
        <label for="password-signup">Password:</label>
        <input class="form-input" type="password" id="password-signup" />
    </div>
    <div class="form-group">
        <button class="btn btn-custom" type="submit">Sign Up</button>
    </div>
    </form>
    <button class="btn" type="submit"><a href="/login">Already have an account?</a></button>
</div>

<script src="/js/signup.js"></script>