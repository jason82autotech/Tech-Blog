<div class="col-md-12">
  <h2>Login</h2>
  <form class="form login-form">
    <div class="form-group">
      <label for="user-login">Username:</label>
      <input class="form-input" type="text" id="user-login" />
    </div>
    <div class="form-group">
      <label for="password-login">Password:</label>
      <input class="form-input" type="password" id="password-login" />
    </div>
    <div class="form-group">
      <button class="btn btn-custom" type="submit">Login</button>
    </div>
  </form>
  <button class="btn" type="submit"><a href="/signup">No account? Sign up</a></button>
</div>

<script src="/js/login.js"></script>